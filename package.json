{
  "name": "vue-ts-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "mock": "npm run mock-server & vue-cli-service serve --mode mock",
    "dev": "vue-cli-service serve ",
    "start": "npm run dev",
    "build-sit": "cross-env NODE_ENV=sit vue-cli-service build",
    "build-uat": "vue-cli-service build --mode uat",
    "build-prod": "vue-cli-service build",
    "mock-server": "cd mock && tsnd express-mock-server.ts",
    "svg": "vsvg -s ./src/icons/svg -t ./src/icons/components --ext ts --es6",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "cross-env": "^5.2.0",
    "element-ui": "^2.10.1",
    "express": "^4.17.1",
    "js-cookie": "^2.2.0",
    "cors": "^2.8.5",
    "jsencrypt": "^3.0.0-rc.1",
    "morgan": "^1.9.1",
    "node.extend": "^2.0.2",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "tslint-config-airbnb": "^5.11.1",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.3",
    "vue-svgicon": "^3.2.6",
    "vuex": "^3.0.1",
    "vuex-module-decorators": "^0.9.9"
  },
  "devDependencies": {
    "@types/yamljs": "^0.2.30",
    "@types/faker": "^4.1.5",
    "@types/jest": "^23.1.4",
    "@types/js-cookie": "^2.2.2",
    "@types/nprogress": "^0.2.0",
    "@types/express": "^4.17.0",
    "@types/compression": "^0.0.36",
    "@types/cors": "^2.8.5",
    "@types/morgan": "^1.7.35",
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-typescript": "^3.9.0",
    "@vue/cli-plugin-unit-jest": "^3.9.0",
    "@vue/cli-service": "^3.9.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "chalk": "2.4.2",
    "chokidar": "2.1.5",
    "yamljs": "^0.3.0",
    "faker": "^4.1.0",
    "concurrently": "^4.1.1",
    "swagger-routes-express": "^3.0.2",
    "babel-core": "7.0.0-bridge.0",
    "lint-staged": "^8.1.5",
    "node-sass": "^7.0.0",
    "sass-loader": "^7.1.0",
    "ts-jest": "^23.0.0",
    "ts-node-dev": "^1.0.0-pre.40",
    "typescript": "^3.4.3",
    "vue-template-compiler": "^2.6.10"
  }
